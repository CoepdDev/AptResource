@model IEnumerable<AptEMS.Models.Rejected>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_AdminDashboard.cshtml";
}

<h2>Rejected List </h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
